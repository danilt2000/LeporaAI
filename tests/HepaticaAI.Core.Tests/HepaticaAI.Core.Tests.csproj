		<ImplicitUsings>enable</ImplicitUsings>
		<Nullable>enable</Nullable>
	</PropertyGroup>

	<ItemGroup>
	  <Content Include="..\..\src\HepaticaAI.Web\appsettings.json" Link="appsettings.json">
